{
  "name": "zobaken/apd",
  "description": "Simple PHP api documentation generator",
  "version": "0.3",
  "keywords": [
    "php",
    "api",
    "documentation",
    "generator",
    "html"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Nikolay Neizvesny",
      "email": "zobaken@gmail.com",
      "role": "Creator"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "cebe/markdown": "~1.0.1"
  },
  "bin": [
    "bin/apd-html.php",
    "bin/apd-markdown.php",
    "bin/apd-text.php"
  ],
  "autoload": {
    "files": [
      "bootstrap.php"
    ],
    "psr-4": {
      "Apd\\": "src/"
    }
  }
